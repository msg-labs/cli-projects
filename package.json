    "bugs": {
        "url": "https://github.com/msg-labs/cli-projects/issues"
    },
    "homepage": "https://github.com/msg-labs/cli-projects#readme",
    "devDependencies": {
        "@msg-labs/eslint-config": "1.6.0",
